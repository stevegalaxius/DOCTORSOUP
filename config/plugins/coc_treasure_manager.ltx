; This plugin configures the coc_treasure_manager script file.

; Ensures that a minimal amount of certain items will be spawned.
[always_in_existence]
af_compass = 1
addons_box = 0
detector_advanced = 0
detector_elite = 0
detector_scientific = 0
detector_simple = 0
equ_military_pack = 5
equ_tourist_pack = 3
itm_repairkit_tier_1 = 7
itm_repairkit_tier_2 = 5
itm_repairkit_tier_3 = 3
wpn_gauss = 1

; These are 'bad' stashes, i.e. boxes which are unreachable.
[ignore_stashes]
